{
  "rewrites": [
    {
      "source": "/nevado-toluca",
      "destination": "/nevado-toluca.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}